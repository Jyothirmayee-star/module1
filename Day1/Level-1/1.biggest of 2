#include <stdio.h>
int main() {
    int num1,num2;// biggest of two numbers using if else
    printf("Enter num1:");
    scanf("%d",&num1);
    printf("Enter num2:");
    scanf("%d",&num2);
    if(num1==num2)
    printf("Both are equal");
    else if(num1>num2)
    printf("%d is greater",num1);
    else
    printf("%d is greater",num2);
    return 0;
}
